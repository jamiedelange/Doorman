<table>
<thead>
  <tr>
            <th>Room #</th>
            <th>Patient Name</th>
            <th>COVID Positive [Y/N]</th>
            <th>Final Visit [Y/N]</th>
            <th>Visitor Name and Phone #</th>
  </tr>
</thead>
<tbody>
  {{#each patients}}
    <tr>
    	{{#each this}}
        <td class="tableField">{{this}}</td>
      {{/each}}
    </tr>
  {{/each}}
</tbody>
</table>
<form id="add-patient-form">
  <label for="patient-room-number">Patient room number:</label>
  <input type="text" class="add-patient" id="patient-room-number"/>

  <label for="patient-name">Patient name:</label>
  <input type="text" id="patient-name"/>

  <label for="patient-covid-positive">COVID Positive (Y/N):</label>
  <input type="text" class="add-patient" id="patient-covid-positive"/>

  <label for="patient-final-visit">Final visit (Y/N):</label>
  <input type="text" class="add-patient" id="patient-final-visit"/>

  <button class="add-btn" type="submit">Add patient</button>
</form>
<script src="javascript/add-patient.js"></script>